{
    "build": {
      "core": "stm32",
      "cpu": "cortex-m4",
      "extra_flags": "-DSTM32G4xx -DSTM32G491xx -DARDUINO_GENERIC_G491CETX -DENABLE_HWSERIAL1 -DPIN_SERIAL1_RX=PA10 -DPIN_SERIAL1_TX=PA9 -DENABLE_HWSERIAL2 -DPIN_SERIAL2_RX=PA3 -DPIN_SERIAL2_TX=PA2 -DENABLE_HWSERIAL3 -DPIN_SERIAL3_RX=PB11 -DPIN_SERIAL3_TX=PB10",
      "f_cpu": "150000000L",
      "mcu": "stm32g491cet6",
      "product_line": "STM32G491xx",
      "variant": "G491C(C-E)T_G4A1CET",
      "variants_dir": "variants",
          "arduino": {
              "variant_h": "variant_generic.h"
          }
    },
    "connectivity": [
      "can"
    ],
    "debug": {
      "default_tools": [
        "stlink"
      ],
      "jlink_device": "STM32G491CE",
      "onboard_tools": [
        "stlink"
      ],
      "openocd_target": "stm32g4x",
      "svd_path": "STM32G491xx.svd"
    },
    "frameworks": [
      "arduino",
      "cmsis",
      "libopencm3",
      "stm32cube"
    ],
    "name": "crash-n-burn",
    "upload": {
      "maximum_ram_size": 131072,
      "maximum_size": 524288,
      "protocol": "dfu", 
      "protocols": [
        "stlink",
        "jlink",
        "cmsis-dap",
        "blackmagic",
        "mbed"
      ]
    },
    "url": "https://www.st.com/en/microcontrollers-microprocessors/stm32g491ce.html",
    "vendor": "UB SEDS"
  }
  